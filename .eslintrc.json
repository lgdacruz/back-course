{
 "env": {
  "es2021": true,
  "node": true
 },
 "extends": ["airbnb-base", "prettier"],
 "parser": "@typescript-eslint/parser",
 "parserOptions": {
  "ecmaVersion": "latest",
  "sourceType": "module"
 },
 "plugins": ["@typescript-eslint"],
 "rules": {
  "rules": {
   "react/jsx-filename-extension": [
    1,
    { "extensions": [".js", ".jsx", ".ts", "tsx"] }
   ],
   "import/extensions": "off",
   "import/no-unresolved": "off"
  }
 }
}
